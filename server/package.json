{
  "name": "schedule-parser-server",
  "version": "1.0.0",
  "description": "Server for university schedule",
  "main": "index.ts",
  "scripts": {
    "lint": "npx eslint ./src",
    "format": "npx eslint ./src --fix",
    "build": "tsc && tsc-alias",
    "start": "node dist/index",
    "dev": "nodemon",
    "delete": "ts-node -r tsconfig-paths/register src/database/scripts/deleteAllGroups",
    "download": "ts-node -r tsconfig-paths/register src/database/scripts/downloadSchedules",
    "update": "ts-node -r tsconfig-paths/register src/database/scripts/updateGroups",
    "create": "npm run delete && npm run download && ts-node -r tsconfig-paths/register src/database/scripts/createGroups",
    "test": "ts-node -r tsconfig-paths/register src/api/getProcessedDataForFile"
  },
  "keywords": [],
  "author": "Matvey Molchanov",
  "license": "ISC",
  "dependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.1.1",
    "express": "^4.19.2",
    "mongodb": "^6.6.2",
    "mongoose": "^8.4.0",
    "node-html-parser": "^6.1.13",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "nodemon": "^3.1.0"
  }
}
